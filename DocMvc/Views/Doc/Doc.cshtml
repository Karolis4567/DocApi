@model DocModel

<div class="container">
    <div class="row">
        <div class="col-sm-4">
            <div class="flex-shrink-0 p-3 bg-white" style="width: 280px;">
                <a href="/" class="d-flex align-items-center pb-3 mb-3 link-dark text-decoration-none border-bottom">
                    <svg class="bi pe-none me-2" width="30" height="24"><use xlink:href="#bootstrap" /></svg>
                    <span class="fs-5 fw-semibold">Contents:</span>
                </a>
                <ul class="list-unstyled ps-0">
                    <li class="mb-1">
                        <button class="btn btn-toggle d-inline-flex align-items-center rounded border-0 collapsed" data-bs-toggle="collapse" data-bs-target="#home-collapse" aria-expanded="true">
                            Controllers
                        </button>
                        <div class="collapse show" id="home-collapse">
                            <ul class="btn-toggle-nav list-unstyled fw-normal pb-1 small">
                                @foreach(string item in Model.controllerNames)
                                {
                                    <li>
                                        <a href="#" class="link-dark d-inline-flex text-decoration-none rounded">
                                            @item
                                        </a>
                                    </li>

                                }

                                
                            </ul>
                        </div>
                    </li>
                   
                    <li class="mb-1">
                        <button class="btn btn-toggle d-inline-flex align-items-center rounded border-0 collapsed" data-bs-toggle="collapse" data-bs-target="#orders-collapse" aria-expanded="false">
                            Objects 
                        </button>
                        <div class="collapse" id="orders-collapse">
                            <ul class="btn-toggle-nav list-unstyled fw-normal pb-1 small">
                                @foreach(string item in Model.objectNames)
                                {
                                    <li>
                                        <a href="#" class="link-dark d-inline-flex text-decoration-none rounded">
                                            @item
                                        </a>
                                    </li>
                                }
                            </ul>
                        </div>
                    </li>                
                </ul>
            </div>


        </div>
        <div class="col-sm-8">
            <h1>
               <h1><u>@Model.title</u></h1>
               <h6>Version: @Model.version, Swagger: @Model.swagger</h6>
            </h1>
        </div>
        
    </div>
</div>


